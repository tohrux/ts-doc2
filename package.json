{
  "name": "ts-doc2",
  "version": "1.0.14",
  "description": "Generate api doc by decorator",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc -p build.tsconfig.json --outDir .",
    "build:view":"cd src/view && npm run build",
    "echo":"echo test"
  },
  "keywords": [
    "typescript",
    "api",
    "doc"
  ],
  "files": [
    "/src/genSchema",
    "common*",
    "index.js"
  ],
  "bin": {
    "ts-doc2": "src/index.js"
  },
  "main": "index.js",
  "author": "d1vaainiyo@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^18.0.0",
    "@types/prettier": "^2.6.3",
    "commander": "^9.4.0",
    "prettier": "^2.7.1",
    "ts-morph": "^14.0.0",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "typescript-json-schema": "^0.53.0"
  }
}
